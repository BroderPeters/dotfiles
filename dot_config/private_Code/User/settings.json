{
    "workbench.activityBar.visible": true,
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "workbench.startupEditor": "newUntitledFile",
    "files.associations": {
        "*.csv": "csv (semicolon)",
        "*.zpl": "ignore"
    },
    "terminal.explorerKind": "external",
    "editor.formatOnSave": true,
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext"
    ],
    "window.titleBarStyle": "custom",
    "spellright.language": [
        "en_US"
    ],
    "spellright.suggestionsInHints": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": false,
    "explorer.confirmDelete": true,
    "breadcrumbs.enabled": true,
    "editor.renderWhitespace": "none",
    "csproj.excludeRegex": "*.cs",
    "workbench.sideBar.location": "left",
    "liveshare.showInStatusBar": "whileCollaborating",
    "editor.wordWrap": "on",
    "terminal.integrated.shell.linux": "/usr/bin/zsh",
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false,
        "delay": 500,
        "exclude": [
            "**/*"
        ]
    },
    "todo-tree.general.statusBar": "top three",
    "todo-tree.highlights.enabled": true,
    "editor.rulers": [
        120
    ],
    "terraform.telemetry.enabled": false,
    "omnisharp.loggingLevel": "warning",
    "vssolution.xmlClosingTagSpace": true,
    "[vb]": {
        "files.encoding": "windows1252"
    },
    "todo-tree.tree.showScanModeButton": false,
    "aws.profile": "profile:dev",
    "[csharp]": {
        "editor.defaultFormatter": "ms-dotnettools.csharp"
    },
    "rust-client.rustupPath": "/home/bp/.cargo/bin/rustup",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.editor.untitled.hint": "hidden",
    "omnisharp.enableRoslynAnalyzers": true,
    "window.zoomLevel": 1
}